#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/.pnpm/registry.npmjs.org/nodemon/2.0.1/node_modules/nodemon/bin/nodemon.js/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/.pnpm/registry.npmjs.org/nodemon/2.0.1/node_modules/nodemon/bin/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/.pnpm/registry.npmjs.org/nodemon/2.0.1/node_modules/nodemon/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/.pnpm/registry.npmjs.org/nodemon/2.0.1/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/.pnpm/registry.npmjs.org/nodemon/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/.pnpm/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/node_modules:/Users/vinnihoke/git-projects/node_modules:/Users/vinnihoke/node_modules:/Users/node_modules:/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/nodemon/bin/nodemon.js/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/nodemon/bin/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/nodemon/node_modules" "$basedir/node"  "$basedir/../nodemon/bin/nodemon.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/.pnpm/registry.npmjs.org/nodemon/2.0.1/node_modules/nodemon/bin/nodemon.js/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/.pnpm/registry.npmjs.org/nodemon/2.0.1/node_modules/nodemon/bin/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/.pnpm/registry.npmjs.org/nodemon/2.0.1/node_modules/nodemon/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/.pnpm/registry.npmjs.org/nodemon/2.0.1/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/.pnpm/registry.npmjs.org/nodemon/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/.pnpm/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/node_modules:/Users/vinnihoke/git-projects/node_modules:/Users/vinnihoke/node_modules:/Users/node_modules:/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/nodemon/bin/nodemon.js/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/nodemon/bin/node_modules:/Users/vinnihoke/git-projects/netninja-tutorials/graphql/curriculum/node_modules/nodemon/node_modules" node  "$basedir/../nodemon/bin/nodemon.js" "$@"
